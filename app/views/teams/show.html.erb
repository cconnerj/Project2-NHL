<div class="teamtitle">
<h1><%= "#{@team.city} #{@team.name}" %></h1>
<%= image_tag @team.img_url %>
<p><%= link_to "Edit Team Information", edit_team_path %>
<p><%= link_to "Add Player to team", team_players_path(@team) %></p>
<% @team.players.each do |player| %>
</div>
<div class = "teamplayer">

  <h3><%= link_to "#{player.name} (#{player.position})", team_player_path(@team, player) %></h3>

  <%= image_tag player.img_url %>

</div>
<% end %>
